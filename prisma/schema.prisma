generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}


model stores {
  shop     String   @id
  isActive Boolean? @default(false)
}

model session {
  id      String  @id
  content String? @db.Text
  shop    String?
}
